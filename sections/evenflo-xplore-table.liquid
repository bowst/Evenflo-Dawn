  <div class="rich-text__content center">
    <h2 class="rich-text__heading title">FIND THE BEST SPINNY FOR YOUR MINI</h2>
  </div>
  <table class="comp-table" id="revolve">
    <caption>
      {{ section.settings.title }}
    </caption>
    <thead class="comp-table__tableHead">
      <tr class="comp-table__columnHeaders comp-table__tableRow">
        <th
          title="Empty placeholder cell"
          class="comp-table__emptyCell comp-table__tableData comp-table__columnHeader"
        ></th>
        <th scope="col" class=" comp-table__tableData comp-table__columnHeader">
          <div class="comp-table__buyProduct">
            <img src="https://cdn.shopify.com/s/files/1/0512/2359/3116/files/revolve360.png?v=1687438812">
            <p class="comp-table__productName">Revolve360</p>

            <p class="product-name" data-price-1="Easy Ins and Outs">Easy Ins and Outs</p>
          </div>
        </th>
        <th scope="col" class="comp-table__recommended comp-table__tableData comp-table__columnHeader">
          <div class="comp-table__buyProduct">
            <img src="https://cdn.shopify.com/s/files/1/0512/2359/3116/files/revolve360-extend.png?v=1687438811">
            <span class="comp-table__productName">Revolve360 Extend</span>

            <p class="product-name" data-price-1="Extended rear-facing to 50 LB" data-price-2="$500">
              Extended rear-facing to 50 LB
            </p>
          </div>
        </th>
        <th scope="col" class=" comp-table__tableData comp-table__columnHeader">
          <div class="comp-table__buyProduct">
            <img src="https://cdn.shopify.com/s/files/1/0512/2359/3116/files/revolve360-slim.png?v=1687438811">
            <span class="comp-table__productName">Revolve360 Slim</span>
            <p class="product-name">Backseat Space Saving</p>
          </div>
        </th>
      </tr>
    </thead>
