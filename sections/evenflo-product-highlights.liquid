{{ 'section-product-highlights.css' | asset_url | stylesheet_tag }}

<!-- child weight height and age section under the product images that will be here for every type of product -->
{% assign child_weight_min_value = product.metafields.sf_products.child_weight_min %}
{% assign child_weight_max_value = product.metafields.sf_products.child_weight_max %}
{% assign child_height_min_value = product.metafields.sf_products.child_height_min %}
{% assign child_height_max_value = product.metafields.sf_products.child_height_max %}
{% assign lightweight = product.metafields.sf_car_seat.lightweight %}
{% assign anti_rebound_bar = product.metafields.sf_car_seat.anti_rebound_bar %}
{% assign package_contents = product.metafields.sf_products.retail_package_contents %}
{% assign assembled_width = product.metafields.sf_products.assembled_width %}
{% assign assembled_height = product.metafields.sf_products.assembled_height %}
{% assign assembled_depth = product.metafields.sf_products.assembled_depth %}
{% assign package_width = product.metafields.sf_shipping.package_width %}
{% assign package_height = product.metafields.sf_shipping.package_height %}
{% assign package_depth = product.metafields.sf_shipping.package_depth %}
{% assign folded_dimensions = product.metafields.sf_products.folded_dimensions %}
{% assign accepts_infant_car_seat = product.metafields.sf_stroller.accepts_infant_car_seat %}
{% assign wheel_material = product.metafields.sf_stroller.wheel_material %}
{% assign modes_of_use = product.metafields.custom.modes_of_use | replace: '|', '<br>' %}
{% assign installation = product.metafields.custom.installation %}
{% assign number_of_cup_holders = product.metafields.sf_products.number_of_cup_holders %}
{% assign converts_to_no_back_booster = product.metafields.sf_car_seat.converts_to_no_back_booster %}
