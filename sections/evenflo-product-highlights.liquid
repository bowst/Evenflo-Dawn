{% assign machine_washable = product.metafields.sf_products.machine_washable_seating_surface %}
{% assign made_usa = product.metafields.sf_products.made_in_usa %}
{% assign harness_type = product.metafields.sf_products.harness_type %}
{% assign height_adjustable = product.metafields.sf_high_chair.height_adjustable %}
{% assign suction_range = product.metafields.custom.suction_range %}
{% assign tip_styles = product.metafields.custom.tip_styles %}
{% assign features = product.metafields.product.features %}
{% assign material = product.metafields.sf_products.material %}

{% assign accepts_rider_board = product.metafields.sf_stroller.accepts_rider_board %}
{% assign has_harness_booster_mode = product.metafields.sf_car_seat.has_harness_booster_mode %}

{% assign battery_cell_type = product.metafields.sf_activity.battery_cell_type %}

<section class="product-highlights anchor-gradient">
  <div class="content-container page-width color-accent-4 rich-text--full-width content-container--full-width">
    {% comment %} All in One {% endcomment %}
    {% if template == 'product.all-in-one-car-seat' %}
      <div class="flex-row">
        <div class="flex-item">
          <h3>Child Fit - Rear Facing</h3>

          <strong>Weight:</strong>
          {{ product.metafields.sf_car_seat.rear_facing_weight_min }} -
          {{ product.metafields.sf_car_seat.rear_facing_weight_max }} lbs <br>
          <strong>Height:</strong>
          {{ product.metafields.sf_car_seat.rear_facing_height_min }} -
          {{ product.metafields.sf_car_seat.rear_facing_height_max }} lbs <br>
        </div>
        <div class="flex-item">
          <h3>Child Fit - Forward Facing</h3>
          <strong>Weight:</strong>
          {{ product.metafields.sf_car_seat.forward_facing_weight_min }} -
          {{ product.metafields.sf_car_seat.forward_facing_weight_max }} lbs <br>
          <strong>Height:</strong>
          {{ product.metafields.sf_car_seat.forward_facing_height_min }} -
          {{ product.metafields.sf_car_seat.forward_facing_height_max }} lbs <br>
        </div>

        <div class="flex-item">
          <h3>Child Fit - Booster</h3>
          <strong>Weight:</strong>
          {{ product.metafields.sf_car_seat.belt_pos_booster_weight_min }} -
          {{ product.metafields.sf_car_seat.belt_pos_booster_weight_max }} lbs <br>
          <strong>Height:</strong>
          {{ product.metafields.sf_car_seat.belt_pos_booster_height_min }} -
          {{ product.metafields.sf_car_seat.belt_pos_booster_height_max }} lbs <br>
        </div>
        <div class="flex-item">
          <h3>Product Dimensions</h3>
          <strong>Assembled:</strong> {{ assembled_width }} x {{ assembled_height }} x {{ assembled_depth }} in<br>
          <strong>Package:</strong> {{ package_width }} x {{ package_height }} x {{ package_depth }} in<br>
        </div>
        <div class="flex-item">
          <h3>Modes of Use</h3>
          <strong>{{ modes_of_use }}</strong>
        </div>
        <div class="flex-item">
          <h3>Specs</h3>
          {% assign my_switch = product.metafields.custom.rotation %}
          {% if my_switch == true %}
            <strong>360 Rotation</strong><br>
          {% endif %}
          <strong> {{ product.metafields.custom.installation }} Installation</strong>
          <br>
          {% assign my_switch = product.metafields.sf_car_seat.steel_reinforced %}
          {% if my_switch == 1 %}
            <strong>Steel Reinforced</strong>
          {% endif %}

          <br>
          {% assign my_switch = product.metafields.sf_car_seat.anti_rebound_bar %}
          {% if my_switch == 1 %}
            <strong>Anti Rebound Bar</strong>
          {% endif %}
        </div>
      </div>
    {% endif %}

    {% comment %} Baby Proofing {% endcomment %}
    {% if template == 'product.baby-proofing' %}
      <div class="flex-row">
        <div class="flex-item">
          <h3>Product Dimensions</h3>
          <strong>Dimensions:</strong><br>
          {{ assembled_width }} x {{ assembled_height }} x {{ assembled_depth }} in <br>
        </div>
        <div class="flex-item">
          <h3>Warranty Type</h3>
          {% assign product_warranty_link = product.metafields.custom.product_warranty_link %}
          {% if product_warranty_link %}
            {% if product.metafields.sf_products.warranty == '1 Year, 6 Month Battery' %}
              <a href="{{ product_warranty_link }}">Limited 1 Year, 6-Month Battery Warranty</a><br>
            {% endif %}
          {% endif %}
          {% if product.metafields.sf_products.warranty == 'lifetime-limited' %}
            <a href="https://www.evenflo.com/pages/warranty">Limited Lifetime Warranty</a><br>
          {% endif %}
          {% if product.metafields.sf_products.warranty == '1 Year, Limited' %}
            <a href="https://www.evenflo.com/pages/warranty">Limited 1 Year Warranty</a> <br>
          {% endif %}
          {% if product.metafields.sf_products.warranty == '90-day-limited' %}
            <a href="{{ product_warranty_link }}">Limited 90-Day Warranty</a><br>
          {% endif %}
        </div>
        <div class="flex-item">
          <h3>Specs</h3>
          <strong>{{ package_contents }}</strong><br>
          <strong>Tested To {{ product.metafields.custom.tested_to }} lbs</strong><br>
          <strong>{{ product.metafields.custom.mount_type }}</strong><br>
        </div>
      </div>
      <div>
        <a href="{{ product.metafields.custom.support_page_link }}" class="see-more">SEE MORE >></a>
      </div>
    {% endif %}

