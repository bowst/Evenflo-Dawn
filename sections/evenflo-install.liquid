{{ 'section-full-width.css' | asset_url | stylesheet_tag }}

<section class="full-width">
  <div class="content-container page-width rich-text--full-width content-container--full-width">
    <h2 class="h1 heading-underline">{{ section.settings.heading }} </h2>
    <div class="product__description rte quick-add-hidden" >
    {{ section.settings.content }} 
    </div>

{%- for block in section.blocks -%}
  {%- case block.type -%}

    {%- when 'videos' -%}
      {%- # Label: Html 0 -%}
      {{ block.settings.html_0 }} 

  {%- endcase -%}
{%- endfor -%}
  </div>
</section>
{% schema %}
  {
  "name": "Evenflo Install",
  "class": "text",
  "tag": "section",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "Install/Setup",
      "placeholder": "Install/Setup"
    },
    {
      "type": "textarea",
      "id": "content",
      "label": "Content"
    }
  ],
  "blocks": [
    {
      "name": "Videos",
      "type": "videos",
      "settings": [
        {
          "type": "html",
          "label": "Html 0",
          "id": "html_0"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Evenflo Install"
    }
  ]
}  
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
{% endjavascript %}