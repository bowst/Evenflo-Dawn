    "48d29db5-9c40-4e0e-90f2-26ac041f1491": {
      "type": "image-with-text",
      "disabled": true,
      "blocks": {
        "template--15781023842460__48d29db5-9c40-4e0e-90f2-26ac041f1491-1683734782013e458e-0": {
          "type": "heading",
