      "type": "category_anchors",
      "disabled": true,
      "settings": {
        "category_anchor": "Shop Strollers",
        "html": "<div class=\"flex-container\">\n  <a href=\"\/collections\/travel-systems\" class=\"flex-item\">Travel Systems<\/a>\n  <a href=\"\/collections\/strollers\" class=\"flex-item\">Strollers<\/a>\n  <a href=\"\/collections\/car-seat-stroller-combos\" class=\"flex-item\">Car Seat Stroller Combo<\/a>\n  <a href=\"\/collections\/stroller-accessories\" class=\"flex-item\">Accessories<\/a>\n<\/div>",
